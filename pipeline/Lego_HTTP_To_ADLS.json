{
	"name": "Lego_HTTP_To_ADLS",
	"properties": {
		"description": "Copy Themes.csv file from Lego dataset into ADSL container named Lego",
		"activities": [
			{
				"name": "Copy Themes From HTTP to ADLS",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "@{pipeline().parameters.FileName}"
					},
					{
						"name": "Destination",
						"value": "lego//@{pipeline().parameters.FileName}.csv"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Warning",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "ABLB_Lego",
								"type": "LinkedServiceReference"
							},
							"path": {
								"value": "lego/errors/lego//@{pipeline().parameters.FileName}.csv",
								"type": "Expression"
							}
						}
					}
				},
				"inputs": [
					{
						"referenceName": "HTTP_Lego",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "@pipeline().parameters.FileName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_Lego",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "@pipeline().parameters.FileName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"FileName": {
				"type": "String",
				"defaultValue": "FileName"
			}
		},
		"folder": {
			"name": "Lego"
		},
		"annotations": [],
		"lastPublishTime": "2021-02-21T18:39:55Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}