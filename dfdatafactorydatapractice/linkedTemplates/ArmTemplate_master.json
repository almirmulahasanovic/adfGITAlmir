{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfdatafactorydatapractice"
		},
		"ABLB_Lego_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABLB_Lego'"
		},
		"ADLS_Lego_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_Lego'"
		},
		"ASQL_Lego_Entertainment_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ASQL_Lego_Entertainment'"
		},
		"BlobLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobLS'"
		},
		"DataLakeSinkLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeSinkLS'"
		},
		"DestinationADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DestinationADLS'"
		},
		"IMDb_Source_LinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'IMDb_Source_LinkedService'"
		},
		"LinkedServiceToBlobSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedServiceToBlobSource'"
		},
		"Sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Sink'"
		},
		"ADLS_Lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsdatapractice.dfs.core.windows.net"
		},
		"DataLakeSinkLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsdatapractice.dfs.core.windows.net"
		},
		"DestinationADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsdatapractice.dfs.core.windows.net"
		},
		"HTTP_Lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"Sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsdatapractice.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfdatafactorydatapractice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABLB_Lego_connectionString": {
						"value": "[parameters('ABLB_Lego_connectionString')]"
					},
					"ADLS_Lego_accountKey": {
						"value": "[parameters('ADLS_Lego_accountKey')]"
					},
					"ASQL_Lego_Entertainment_connectionString": {
						"value": "[parameters('ASQL_Lego_Entertainment_connectionString')]"
					},
					"BlobLS_connectionString": {
						"value": "[parameters('BlobLS_connectionString')]"
					},
					"DataLakeSinkLS_accountKey": {
						"value": "[parameters('DataLakeSinkLS_accountKey')]"
					},
					"DestinationADLS_accountKey": {
						"value": "[parameters('DestinationADLS_accountKey')]"
					},
					"IMDb_Source_LinkedService_connectionString": {
						"value": "[parameters('IMDb_Source_LinkedService_connectionString')]"
					},
					"LinkedServiceToBlobSource_connectionString": {
						"value": "[parameters('LinkedServiceToBlobSource_connectionString')]"
					},
					"Sink_accountKey": {
						"value": "[parameters('Sink_accountKey')]"
					},
					"ADLS_Lego_properties_typeProperties_url": {
						"value": "[parameters('ADLS_Lego_properties_typeProperties_url')]"
					},
					"DataLakeSinkLS_properties_typeProperties_url": {
						"value": "[parameters('DataLakeSinkLS_properties_typeProperties_url')]"
					},
					"DestinationADLS_properties_typeProperties_url": {
						"value": "[parameters('DestinationADLS_properties_typeProperties_url')]"
					},
					"HTTP_Lego_properties_typeProperties_url": {
						"value": "[parameters('HTTP_Lego_properties_typeProperties_url')]"
					},
					"Sink_properties_typeProperties_url": {
						"value": "[parameters('Sink_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dfdatafactorydatapractice_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dfdatafactorydatapractice_ArmTemplate_0')]"
			]
		}
	]
}